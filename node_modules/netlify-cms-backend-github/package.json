    "js-base64": "^2.5.1",
    "semaphore": "^1.1.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "lodash": "^4.17.11",
    "netlify-cms-lib-auth": "^2.2.0",
