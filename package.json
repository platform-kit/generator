    "v-markdown": "^1.0.2",
    "v-runtime-template": "^1.10.0",
    "vue": "^2.6.11",
    "vue-plyr": "^6.0.4",
    "vue-prism": "^1.0.5",
    "vue-stripe-checkout": "^3.3.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.0"
  },
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "platformkit",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "version": "0.0.6"
}
